{
  "env": {
    "browser": true,
    "es2021": true
  },
  "globals": {
    "ga": true,
    "chrome": true
  },
  "extends": ["plugin:vue/essential", "standard"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "semi": [0, "warn"],
    "space-before-function-paren":[0, "warn"]
  }
}
